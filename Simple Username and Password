#include <iostream>
#include <limits>
#include <iomanip>

using namespace std;

int main() {
	string Newusername;
	cout << "Please enter a Username: " << Newusername << flush;
	cin >> Newusername;

	string Newpassword;
	cout << "Please enter a new password: " << Newpassword << flush;
	cin >> Newpassword;

	cout << "Log in" << endl;
	string username;
	cout << "Username:" << username << flush;
	cin >> username;

	string password;
	cout << "Password: " << password << flush;
	cin >> password;

	if ((Newusername == username) && (Newpassword == password)) {
		cout << "WELCOME BACK!" << flush;
	} else {
		cout << "ACESS DENIED" << flush;
	}

	return 0;
}
